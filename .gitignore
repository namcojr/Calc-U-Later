# Android/IntelliJ/Gradle/Kotlin .gitignore

# Built application files
*.apk
*.ap_

# Files for the ART/Dalvik VM
*.dex

# Java class files
*.class

# Generated files
bin/
gen/
out/
build/

# Gradle files
.gradle/
build/

# Local configuration file (sdk path, etc)
local.properties
# Gradle project properties (may contain secrets like signing keys)
gradle.properties

# Proguard folder generated by Eclipse
proguard/

# Log files
*.log

# Android Studio
.idea/
*.iml

# Keystore files
*.jks
*.keystore

# Signing files
signingConfigs/

# Kotlin/IntelliJ
*.kts

# Mac
.DS_Store

# VS Code
.vscode/

# Crashlytics
crashlytics/

# NDK
obj/

# CMake
.cxx/

# Kotlin gradle daemon
.gradle/

# Misc
*.class
*.war
*.ear

# JetBrains
*.ipr
*.iws

# kapt
/kapt*/

# coverage
coverage/

# gradle-wrapper
gradle-wrapper.jar

# Android Studio capture
captures/

# local Android studio settings
.idea/*
!/.idea/gradle.xml

# keep gradle files
!gradle/

# Exclude build generated metadata
**/build/